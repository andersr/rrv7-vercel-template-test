{
  "name": "structured-response-demo",
  "author": {
    "name": "Anders Ramsay",
    "url": "https://www.anders.co/"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andersr/structured-response-demo"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:e2e": "PORT=8080 TEST_ENV=true npm run dev",
    "dev:start": "dotenv -v TEST_ENV=true -- npm start",
    "e2e:start": "dotenv  -v TEST_ENV=true --  npm run build && npm run dev:start",
    "e2e:dev": "npx playwright test --ui",
    "e2e:run": "NODE_ENV=production npx playwright test",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "asst:admin": "dotenv -- tsx app/.server/admin/assistant/main.ts",
    "asst:create": "ACTION=create npm run asst:admin",
    "asst:update": "ACTION=update npm run asst:admin",
    "asst:deploy": "NODE_ENV=production ACTION=update npm run asst:admin",
    "msw:init": "npx msw init"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.2.0",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@upstash/redis": "^1.34.3",
    "express": "^4.21.1",
    "isbot": "^5.1.17",
    "nanoid": "^5.0.9",
    "openai": "^4.79.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.8",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vercel/react-router": "^1.0.2",
    "@vitest/coverage-v8": "^3.0.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.0-beta.8",
    "dotenv-cli": "^8.0.0",
    "encoding": "^0.1.13",
    "jsdom": "^26.0.0",
    "msw": "^2.7.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
