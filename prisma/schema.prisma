// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/triviadb"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Assistant {
  appId String @id // app purpose key,eg homeFeatures
  oaiId String @unique // assistant id generateed by openAi
}
